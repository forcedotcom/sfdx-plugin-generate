{
  "name": "@salesforce/plugin-generator",
  "description": "create your own sfdx plugin",
  "version": "1.1.7",
  "author": "Platform CLI",
  "bin": "./bin/run",
  "bugs": "https://github.com/forcedotcom/sfdx-plugin-generate/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/fixpack": "^2.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-warn-if-update-available": "^1.5.4",
    "debug": "^4.3.1",
    "eslint-config-xo": "^0.36.0",
    "eslint-config-xo-space": "^0.27.0",
    "lodash": "^4.17.20",
    "nps-utils": "^1.7.0",
    "sort-pjson": "^1.0.3",
    "tslib": "^2.2.0",
    "yeoman-environment": "2.4.0",
    "yeoman-generator": "4.0.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/tslint": "^3.1.1",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "^0.9.15",
    "@types/jsforce": "^1.9.29",
    "@types/lodash": "^4.14.170",
    "@types/read-pkg": "^5.1.0",
    "@types/shelljs": "^0.8.3",
    "@types/yeoman-generator": "^3.1.3",
    "@types/yosay": "^2.0.0",
    "chai": "^4.3.4",
    "eslint": "^7.28.0",
    "eslint-config-oclif": "^3.1.0",
    "execa": "^5.1.1",
    "fancy-test": "^1.4.10",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "husky": "^4.3.8",
    "mocha": "^9.0.0",
    "npm-run-path": "^4.0.1",
    "nps": "^5.10.0",
    "shelljs": "^0.8.3",
    "tmp": "^0.2.1",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib",
    "/templates",
    "/messages"
  ],
  "homepage": "https://github.com/forcedotcom/sfdx-plugin-generate",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "topics": {
      "plugins": {
        "description": "add/remove/create CLI plug-ins"
      }
    },
    "commands": "./lib/sfdxcommands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found"
    ],
    "bin": "oclif"
  },
  "repository": "oclif/oclif",
  "scripts": {
    "lint": "nps lint",
    "postpack": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest",
    "version": "oclif-dev readme && git add README.md",
    "test": "nps test",
    "prepare": "rm -f .oclif.manifest.json && yarn run prepack",
    "build": "yarn prepack"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "commit-msg": "sf-husky-commit-msg"
    }
  }
}
