{
  "name": "@salesforce/plugin-generator",
  "description": "Create your own sfdx plugin",
  "version": "2.0.5",
  "author": "Salesforce",
  "bin": "./bin/run",
  "bugs": "https://github.com/forcedotcom/sfdx-plugin-generate/issues",
  "dependencies": {
    "@oclif/core": "^1.16.4",
    "@salesforce/ts-types": "^1.5.21",
    "debug": "^4.3.1",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "sort-pjson": "^1.0.3",
    "tslib": "^2.2.0",
    "yeoman-environment": "^3.12.0",
    "yeoman-generator": "^5.7.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.2.1",
    "@salesforce/cli-plugins-testkit": "^1.5.28",
    "@salesforce/dev-config": "^3.0.1",
    "@salesforce/dev-scripts": "^3.1.0",
    "@salesforce/plugin-command-reference": "^1.3.18",
    "@salesforce/prettier-config": "^0.0.2",
    "@types/lodash": "^4.14.186",
    "@types/shelljs": "^0.8.11",
    "@types/yeoman-environment": "^2.10.8",
    "@types/yeoman-generator": "^5.2.11",
    "@types/yeoman-test": "^4.0.3",
    "@types/yosay": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chai": "^4.3.4",
    "eslint": "^8.21.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "globby": "^11.0.3",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "oclif": "^2.7.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.0",
    "shelljs": "^0.8.3",
    "sinon": "10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "yeoman-test": "^6.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib",
    "/templates",
    "/messages"
  ],
  "homepage": "https://github.com/forcedotcom/sfdx-plugin-generate",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "oclif": {
    "topics": {
      "plugins": {
        "description": "add/remove/create CLI plug-ins"
      }
    },
    "commands": "./lib/sfdxcommands",
    "bin": "oclif",
    "devPlugins": [
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ]
  },
  "repository": "forcedotcom/sfdx-plugin-generate",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000",
    "version": "oclif readme"
  },
  "types": "lib/index.d.ts"
}
