{
  "name": "@salesforce/plugin-generator",
  "description": "create your own sfdx plugin",
  "version": "1.2.2",
  "author": "Platform CLI",
  "bin": "./bin/run",
  "bugs": "https://github.com/forcedotcom/sfdx-plugin-generate/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/fixpack": "^2.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-warn-if-update-available": "^1.5.4",
    "concurrently": "^7.0.0",
    "debug": "^4.3.1",
    "eslint-config-xo": "^0.36.0",
    "eslint-config-xo-space": "^0.27.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "sort-pjson": "^1.0.3",
    "tslib": "^2.2.0",
    "yeoman-environment": "^2.4.0",
    "yeoman-generator": "^4.0.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/tslint": "^3.1.1",
    "@salesforce/dev-config": "^3.0.0",
    "@salesforce/dev-scripts": "^2.0.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@types/jsforce": "^1.9.29",
    "@types/lodash": "^4.14.170",
    "@types/read-pkg": "^5.1.0",
    "@types/shelljs": "^0.8.3",
    "@types/yeoman-generator": "^3.1.3",
    "@types/yosay": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "chai": "^4.3.4",
    "eslint": "^7.28.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "execa": "^5.1.1",
    "fancy-test": "^1.4.10",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "npm-run-path": "^4.0.1",
    "nps": "^5.10.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "shelljs": "^0.8.3",
    "sinon": "10.0.0",
    "tmp": "^0.2.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib",
    "/templates",
    "/messages"
  ],
  "homepage": "https://github.com/forcedotcom/sfdx-plugin-generate",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "topics": {
      "plugins": {
        "description": "add/remove/create CLI plug-ins"
      }
    },
    "commands": "./lib/sfdxcommands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found"
    ],
    "bin": "oclif"
  },
  "repository": "oclif/oclif",
  "scripts": {
    "build": "yarn prepack",
    "lint": "nps lint",
    "postpack": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest",
    "prepare": "rm -f .oclif.manifest.json && yarn run prepack",
    "test": "concurrently --kill-others-on-fail --prefix-colors \"dim\" --prefix \"[{name}]\" --names \"basic\" 'mocha --forbid-only test/commands/plugins-generate/basic.test.js'",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
