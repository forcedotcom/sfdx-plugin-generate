{
  "name": "@salesforce/plugin-generator",
  "description": "create your own sfdx plugin",
  "version": "1.1.2",
  "author": "Platform CLI",
  "bin": "./bin/run",
  "bugs": "https://github.com/forcedotcom/sfdx-plugin-generate/issues",
  "dependencies": {
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.12.6",
    "@oclif/errors": "^1.2.2",
    "@oclif/fixpack": "^2.3.0",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-warn-if-update-available": "^1.5.4",
    "debug": "^4.1.1",
    "lodash": "^4.17.11",
    "nps-utils": "^1.7.0",
    "sort-pjson": "^1.0.3",
    "tslib": "^1.9.3",
    "yeoman-environment": "^2.3.4",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/tslint": "^3.1.1",
    "@types/lodash": "^4.14.121",
    "@types/read-pkg": "^3.0.0",
    "@types/shelljs": "^0.8.3",
    "@types/yeoman-generator": "^3.0.2",
    "@types/yosay": "^0.0.29",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-oclif": "^3.1.0",
    "execa": "^1.0.0",
    "fancy-test": "^1.4.3",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "mocha": "^5.2.0",
    "npm-run-path": "^2.0.2",
    "nps": "^5.9.3",
    "shelljs": "^0.8.3",
    "tmp": "^0.0.33",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib",
    "/templates",
    "/messages"
  ],
  "homepage": "https://github.com/forcedotcom/sfdx-plugin-generate",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "topics": {
      "plugins": {
        "description": "add/remove/create CLI plug-ins"
      }
    },
    "commands": "./lib/sfdxcommands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found"
    ],
    "bin": "oclif"
  },
  "repository": "oclif/oclif",
  "scripts": {
    "lint": "nps lint",
    "postpack": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest",
    "version": "oclif-dev readme && git add README.md",
    "test": "nps test",
    "prepare": "yarn run snyk-protect && rm -f .oclif.manifest.json && yarn run prepack",
    "snyk-protect": "snyk protect"
  },
  "types": "lib/index.d.ts",
  "snyk": true
}
